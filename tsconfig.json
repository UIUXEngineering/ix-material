{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "module": "esnext",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "@uiux/cdk/*": [
        "dist/@uiux/cdk/*"
      ],
      "@uiux/material": [
        "dist/@uiux/material"
      ],
      "@uiux/dal/*": [
        "dist/@uiux/dal/*"
      ],
      "@uiux/fn/*": [
        "dist/@uiux/fn/*"
      ],
      "@uiux/rxjs/*": [
        "dist/@uiux/rxjs/*"
      ],
      "@uiux/services/*": [
        "dist/@uiux/services/*"
      ],
      "@uiux/d3/*": [
        "dist/@uiux/d3/*"
      ]
    }
  }
}
