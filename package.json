    "format.check.libs": "./node_modules/.bin/prettier --config ./.prettierrc --list-different \"libs/**/*{.ts,.js,.scss,.css,.html}\"",
    "format.write.libs": "./node_modules/.bin/prettier --config ./.prettierrc --write \"libs/**/*{.ts,.js,.scss,.css,.html}\"",
