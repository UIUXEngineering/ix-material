{
  "$schema": "../../../node_modules/ng-packagr/package.schema.json",
  "name": "@uiux/services",
  "version": "8.1.6",
  "ngPackage": {
    "dest": "../../../dist/@uiux/services",
    "whitelistedNonPeerDependencies": [
      "immutable",
      "lodash-es",
      "moment",
      "@uiux/rxjs/pipes",
      "@uiux/fn/common",
      "@uiux/fn/object"
    ],
    "lib": {
      "entryFile": "src/public_api.ts",
      "umdModuleIds": {
        "@uiux/rxjs/pipes": "pipes",
        "@uiux/fn/common": "common",
        "@uiux/fn/object": "object"
      }
    }
  },
  "dependencies": {
    "immutable": "3.8.2",
    "lodash-es": "4.17.10",
    "moment": "2.22.1"
  },
  "peerDependencies": {
    "@angular/cdk": "^8.0.0",
    "@angular/common": "^8.0.0",
    "@angular/core": "^8.0.0",
    "@angular/material": "^8.0.0",
    "rxjs": "^6.1.0"
  }
}
